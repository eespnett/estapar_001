@model ManobristaModel
@{
    ViewData["Title"] = "ManobristaCreate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ManobristaCreate</h2>

@using (Html.BeginForm("ManobristaCreate", "Home", FormMethod.Post, new { id = "ManobristaCreate" }))
{

    <div class="form-group">
        <label for="exampleInputEmail1">ID</label>
        @Html.TextBoxFor(model => Model.Id, new { @readonly = "readonly" })
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Descrição</label>
        @Html.TextBoxFor(model => Model.Nome)
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Descrição</label>
        @Html.TextBoxFor(model => Model.CPF)
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Descrição</label>
        @Html.TextBoxFor(model => Model.DtNascimento)
    </div>
    <button type="submit" class="btn btn-primary">Enviar</button>
}