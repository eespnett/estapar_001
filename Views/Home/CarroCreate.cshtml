@model CarroModel
@{
    ViewData["Title"] = "CarroCreate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>CarroCreate</h2>
@using (Html.BeginForm("CarroCreate", "Home", FormMethod.Post, new { id = "CarroCreate" }))
{
    <div class="form-group">
        <label for="exampleInputEmail1">ID</label>
        @Html.TextBoxFor(model => Model.Id, new { @readonly = "readonly" })
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Marcao</label>
        @Html.TextBoxFor(model => Model.MarcaCarro)
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Modelo</label>
        @Html.TextBoxFor(model => Model.ModeloCarro)
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Placa</label>
        @Html.TextBoxFor(model => Model.PlacaCarro)
    </div>
    <button type="submit" class="btn btn-primary">Enviar</button>
}


 
