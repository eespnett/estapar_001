@model TipoManobraModel
@{
    ViewData["Title"] = "TipoManobraCreate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>TipoManobraCreate</h2>

@using (Html.BeginForm("TipoManobraCreate", "Home", FormMethod.Post, new { id = "TipoManobraCreate" }))
{

    <div class="form-group">
        <label for="exampleInputEmail1">ID</label>
        @Html.TextBoxFor(model => Model.Id, new { @readonly = "readonly" })
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Descrição</label>
        @Html.TextBoxFor(model => Model.TipoManobra)
    </div>


    <button type="submit" class="btn btn-primary">Enviar</button>
}


 
